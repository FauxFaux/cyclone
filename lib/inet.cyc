#include <core.h>
#include <arpa/inet.h>
using Core;

namespace Cinet {
  extern "C" {
    int inet_aton(const Cstring cp, struct Inet::in_addr @`r inp);
  }
#define s2c(x) (string_to_Cstring(x))
}

namespace Inet;

int inet_aton(string_t cp, struct in_addr @`r inp) {
  return Cinet::inet_aton(s2c(cp), inp);
}
