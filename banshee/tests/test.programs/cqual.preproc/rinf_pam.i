void pam_window_op(pam_op_kind op, const char *wname, const char *kind)
{
  _IO_putc ('(', stdout);
  switch (op)
    {
    case pam_clear_window: printf("pam-clear-window"); break;
    case pam_change_window: printf("pam-change-window"); break;
    case pam_set_lower_window: printf("pam-set-window-down"); break;
    case pam_set_upper_window: printf("pam-set-window-up"); break;
    case pam_close_window: printf("pam-close-window"); break;
    case pam_split_window: printf("pam-split-window"); break;
    default:
      __fail("rinf_pam.c", 290, __FUNCTION__, "Unexpected window op %d\n", op);
    }
  printf(" (");
  pam_print_escaped_string(wname);
  _IO_putc (' ', stdout);
  pam_print_escaped_string(kind);
  printf("))\n");
}
